# Breakup Recovery Agent - Environment Configuration
# Copy this file to .env and fill in your values

# Optional: Provide a default Gemini API key so users don't need to enter their own
# If not set, users will need to provide their own API key through the UI
# Get your key from: https://makersuite.google.com/app/apikey
DEFAULT_GEMINI_API_KEY=

# Optional: Set to "true" to allow users to override the default API key with their own
# Set to "false" to force using only the default key
ALLOW_USER_API_KEY=true

# Model Configuration
# Gemini model to use for AI agents
GEMINI_MODEL_ID=gemini-2.5-flash-preview-09-2025
# Temperature for response generation (0.0-1.0, higher = more creative)
GEMINI_TEMPERATURE=0.7
# Maximum tokens in response
GEMINI_MAX_TOKENS=2000
